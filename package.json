  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/uuid": "^3.4.6",
    "@types/winston": "^2.4.4",
    "axios": "^0.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "nodemon": "^1.19.4",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
